{
    "name": "karmate/framework",
    "description": "Karmate PHP Flexible Framework",
    "keywords": ["framework", "karmate"],
    "license": "MIT",
    "type": "library",
    "homepage": "https://karmate.org",
    "support": {
        "issues": "https://github.com/karmate/framework/issues",
        "source": "https://github.com/karmate/framework"
    },
    "require": {
        "php": ">=7.0"
    },
    "require-dev": {
        "mockery/mockery": "^1.0",
        "phpunit/phpunit": "~6.0"
    },
    "autoload": {
        "psr-4": {
            "Karmate\\": "src/Karmate"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Karmate\\Tests\\": "tests/Karmate"
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
